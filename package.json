{
    "name": "coop_text_adventure_server",
    "version": "1.0.0",
    "description": "Server for text adventure",
    "main": "index.ts",
    "dependencies": {
        "@types/express": "^4.16.1",
        "@types/node": "^12.0.2",
        "@types/socket.io": "^2.1.2",
        "@typescript-eslint/eslint-plugin": "^1.9.0",
        "@typescript-eslint/parser": "^1.9.0",
        "eslint": "^5.16.0",
        "express": "^4.17.0",
        "http": "^0.0.0",
        "nodemon": "^1.19.0",
        "socket.io": "^2.2.0",
        "ts-node": "^8.1.0",
        "typescript": "^3.4.5"
    },
    "devDependencies": {
        "@typescript-eslint/eslint-plugin": "^1.9.0",
        "@typescript-eslint/parser": "^1.9.0"
    },
    "nodemonConfig": {
        "ignore": [
            "**/*.test.ts",
            "**/*.spec.ts",
            ".git",
            "node_modules"
        ],
        "watch": [
            "src"
        ],
        "exec": "npm start",
        "ext": "ts"
    },
    "scripts": {
        "start": "node --inspect=5858 -r ts-node/register ./index.ts",
        "start:watch": "nodemon",
        "build": "tsc",
        "test": "echo \"Error: no test specified\" && exit 1",
        "lint": "eslint ./src/**/*.ts"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/OkapalDominic/coop_text_adventure.git"
    },
    "author": "",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/OkapalDominic/coop_text_adventure/issues"
    },
    "homepage": "https://github.com/OkapalDominic/coop_text_adventure#readme"
}
